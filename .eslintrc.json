{
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["node"],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "node/no-unsupported-features/node-builtins": "off",
        "node/no-unsupported-features/es-syntax": [
            "error",
            { "version": ">=10.13.0" }
        ],
        "node/no-unsupported-features/es-builtins": [
            "error",
            { "version": ">=10.13.0" }
        ]
    }
}
